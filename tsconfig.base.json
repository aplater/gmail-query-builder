{
  "compilerOptions": {
    "outDir": "./lib",
    "target": "es3",
    "lib": ["es2015"],
    "module": "commonjs",
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  }
}
